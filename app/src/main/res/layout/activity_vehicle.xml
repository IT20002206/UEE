<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Vehicle">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="match_parent"
            android:layout_height="152dp"
            android:layout_marginTop="-22dp"
            android:background="@null"
            app:srcCompat="@drawable/vehicle_details" />

        <ImageButton
            android:id="@+id/imageButton52"
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:srcCompat="@drawable/burger_menu"
            android:layout_marginTop="-60dp"
            android:background="@null"
            android:layout_marginStart="20dp"

            />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="match_parent"
            android:layout_height="126dp"
            app:srcCompat="@drawable/vehicle_orange"
            android:layout_marginTop="20dp"
            />

        <ImageButton
            android:id="@+id/imageButton12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="170dp"
            android:layout_marginTop="-60dp"
            android:background="@null"
            app:srcCompat="@drawable/vehicle_edit" />

        <ImageButton
            android:id="@+id/imageButton21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="270dp"
            android:layout_marginTop="-33dp"
            android:background="@null"
            app:srcCompat="@drawable/vehicle_delete" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="match_parent"
            android:layout_height="126dp"
            android:layout_marginTop="20dp"
            app:srcCompat="@drawable/vehicle_yello" />

        <ImageButton
            android:id="@+id/imageButton22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="170dp"
            android:layout_marginTop="-60dp"
            android:background="@null"
            app:srcCompat="@drawable/vehicle_edit" />

        <ImageButton
            android:id="@+id/imageButton24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="270dp"
            android:layout_marginTop="-33dp"
            android:background="@null"
            app:srcCompat="@drawable/vehicle_delete" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="match_parent"
            android:layout_height="126dp"
            android:layout_marginTop="20dp"
            app:srcCompat="@drawable/vehicle_green" />

        <ImageButton
            android:id="@+id/imageButton25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="170dp"
            android:layout_marginTop="-60dp"
            android:background="@null"
            app:srcCompat="@drawable/vehicle_edit" />

        <ImageButton
            android:id="@+id/imageButton32"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="270dp"
            android:layout_marginTop="-33dp"
            android:background="@null"
            app:srcCompat="@drawable/vehicle_delete" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="35dp"
            android:layout_marginTop="30dp"
            app:srcCompat="@drawable/vehicle_color" />

        <ImageButton
            android:id="@+id/imageButton33"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:background="@null"
            app:srcCompat="@drawable/add_Vehicle"

            />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>